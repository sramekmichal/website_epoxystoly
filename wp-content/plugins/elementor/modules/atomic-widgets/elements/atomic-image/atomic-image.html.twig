{% if settings.image.src is not empty %}
	{% set id_attribute = settings._cssid is not empty ? 'id=' ~ settings._cssid | e('html_attr') : '' %}
	{% if settings.link.href %}
		<a href="https://sramekmichal.github.io/website_epoxystoly/wp-content/plugins/elementor/modules/atomic-widgets/elements/atomic-image/%20settings.link.href%20" class="{{ base_styles['link-base'] }}" target="{{ settings.link.target }}">
	{% endif %}
	<img class="{{ base_styles['base'] }} {{ settings.classes | join(' ') }}" {{ id_attribute }}
		{% for attr, value in settings.image %}
			{% if attr == 'src' %}
				src="https://sramekmichal.github.io/website_epoxystoly/wp-content/plugins/elementor/modules/atomic-widgets/elements/atomic-image/%20value%20|%20e(&#039;full_url&#039;)%20"
			{% else %}
				{{ attr | e('html_attr') }}="{{ value }}"
			{% endif %}
		{% endfor %}
	/>
	{% if settings.link.href %}
		</a>
	{% endif %}
{% endif %}
